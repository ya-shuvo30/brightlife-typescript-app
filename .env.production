# Production Environment Configuration
# For GitHub Pages Deployment with Custom Domain

# VPS Backend Server - HTTPS (SSL Configured)
VITE_API_BASE_URL=https://api.brightlifebd.com/api

# Real Backend (NOT Mock API)
VITE_USE_MOCK_API=false

# App Info
VITE_APP_NAME=BrightLife Bangladesh
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags (can be enabled later)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
